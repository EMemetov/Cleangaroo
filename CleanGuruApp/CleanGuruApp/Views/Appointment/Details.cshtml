
@model Appointment

@{
    ViewData["Title"] = "Details";
}

@if (Model == null)
{
    <h2>No such appointment</h2>
}
else
{
    <div align="right">
        <a asp-action="FutureAppointment" asp-controller="Appointment" class="warning" a>Back to List</a>
    </div>
    <partial name="AppointmentDetail" />
}
